	do -- gameplay
		gameplay:AddToggle({
			text = 'Ball Aura',
			tip = 'instantly get the ball when its close to you',
			callback = funcs.ballAura
		})

		gameplay:AddSlider({
			text = 'range',
			flag = 'ball aura range',
			min = 10,
			max = 50,
			value = 20
		})

		gameplay:AddToggle({
			text = 'Visualizer',
